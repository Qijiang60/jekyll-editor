{
  "manifest_version": 2,
  "name": "Jekyll Editor",
  "short_name": "Jekyll Editor",
  "description": "An markdown editor to write jekyll post.",
  "version": "1.0.0",
  "minimum_chrome_version": "33",
  "icons": {
    "16": "/editor.md/images/logos/editormd-logo-16x16.png",
    "128": "/editor.md/images/logos/editormd-logo-120x120.png"
  },
 "permissions": [
	"storage",
 	"unlimitedStorage",
  "notifications",
	"identity",
	"<all_urls>"
  ],
  "app": {
    "background": {
      "page": "background.html",
      "persisitent": true
    }
  }

}
